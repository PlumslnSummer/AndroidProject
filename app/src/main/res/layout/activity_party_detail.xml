<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home.zhiyuan.tab.ZhiyuanPartyActivity"
    android:orientation="vertical"
    android:background="@color/backgray">

    <include layout="@layout/back_layout"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:background="@color/white"
                android:layout_marginTop="10dp"
                android:id="@+id/tv_party_title"
                android:layout_gravity="center"
                android:gravity="center"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textSize="15sp"
                android:background="@color/white"
                android:layout_marginTop="10dp"
                android:id="@+id/tv_party_jianjie"
                android:layout_gravity="center"
                android:gravity="center"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:background="@color/white"
                android:layout_marginTop="10dp"
                android:id="@+id/tv_party_start"
                android:layout_gravity="center"
                android:gravity="center"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:background="@color/white"
                android:layout_marginTop="10dp"
                android:id="@+id/tv_party_ren"
                android:layout_gravity="center"
                android:gravity="center"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:background="@color/white"
                android:layout_marginTop="10dp"
                android:id="@+id/tv_party_chenban"
                android:layout_gravity="center"
                android:gravity="center"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textSize="15sp"
                android:background="@color/white"
                android:layout_marginTop="10dp"
                android:id="@+id/tv_party_work"
                android:layout_gravity="center"
                android:gravity="center"/>
        </LinearLayout>
    </ScrollView>

</LinearLayout>